algoritmo "semnome"
var
   A, B, C: Real
inicio
A <- (3*3+1)
B <- (2*4+1)
C <- (8*1)
  Escreval(A>B)
  Escreval(B<C)
  Escreval(B>C)
  Escreval(C>A)
  Escreval(A>C)
  Escreval((A>B) e (B<C))
  Escreval((B<A) e (C<B))
  Escreval((C>=B) e (B>=A))
  Escreval((A>B) ou (C>B))
  Escreval((B>A) ou (C>B))
  Escreva((C<B) ou (B<A))

fimalgoritmo
